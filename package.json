{
  "name": "stool",
  "version": "1.0.0",
  "description": "A CouchDB wrapper.",
  "main": "index.ts",
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts}",
      "!**/*.{spec.ts, test.ts}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "coverageDirectory": "coverage"
  },
  "scripts": {
    "build": "",
    "dev": "node_modules/.bin/nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' index.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nowayhecodes/stool.git"
  },
  "keywords": [],
  "author": "Gustavo Cavalcante",
  "license": "GNUv3",
  "bugs": {
    "url": "https://github.com/nowayhecodes/stool/issues"
  },
  "homepage": "https://github.com/nowayhecodes/stool#readme",
  "dependencies": {
    "@types/node": "^14.0.24",
    "tsc": "^1.20150623.0",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/jest": "^26.0.5",
    "babel-jest": "^26.1.0",
    "jest": "^26.1.0",
    "nodemon": "^2.0.4",
    "ts-jest": "^26.1.3",
    "ts-node": "^8.10.2"
  }
}
